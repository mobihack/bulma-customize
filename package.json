{
  "name": "bulma-customize",
  "version": "1.0.0",
  "description": "Create custom Bulma builds",
  "private": true,
  "scripts": {
    "deploy": "gh-pages --dist ./ --src \"{index.html,dist/**}\"",
    "dev": "webpack-dev-server --open --mode=development --config webpack.config.js",
    "prod": "webpack --mode=production --config webpack.config.js",
    "standard": "standard --parser babel-eslint",
    "vnu": "html-validator --file=index.html",
    "test": "npm run standard && npm run prod && npm run vnu"
  },
  "keywords": [
    "customizer",
    "custom",
    "bulma",
    "javascript"
  ],
  "author": "mobihack <mobihack.mee@gmail.com> and Johann-S <johann.servoire@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "bulma": "^0.7.5",
    "jszip": "^3.2.1",
    "postcss": "^7.0.17",
    "sass.js": "^0.10.13",
    "uglifyjs-browser": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "gh-pages": "^2.0.1",
    "glob-all": "^3.1.0",
    "html-validator-cli": "^4.1.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.5.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "standard": {
    "ignore": [
      "src/js/lib/*.js"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
